/*左上下拉列表*/
 page{
     height: 100%;
 }
.quyu{
    position: absolute;
    display: none;
    height: 100%;
    width: 100%;
    z-index: 5;
}
.quyu .qy{
    overflow-y: scroll;
    float: left;
    height: 100%;
    line-height: 80rpx;
    box-sizing: border-box; 
    font-size: 14px;
    color: #717273;
}

.nav{
    width: 180px;
    position: relative;
    z-index: 99;
    display: flex;
    border-top: 1px solid #d1d3d4;
    border-bottom: 1px solid #d1d3d4;
    background: #fff;
}
.nav-son{
    display: flex;
    margin-top: 5px;
    flex: 1;
    text-align: center;
    height: 36px;
    align-items:center;
    justify-content:center;
    font-size: 14px;
}

.borders{
    border-left: 1px solid #e6e6e6;
    border-right: 1px solid #e6e6e6;
}
.content{
    display: inline-block;
}
.content{
    display: -webkit-box;
    text-overflow: ellipsis;
    overflow: hidden; 
    white-space:nowrap; 
}
.icon{
    display: inline-block;
    border: 4px solid transparent;
    border-top: 4px solid #9b9b9b;
    margin-left: 5px;
}
.nav-son.active .content{
    color: #61beff;
}
.nav-son.active .icon{
    border-bottom: 4px solid #fff;
    border-top:0;
}
@keyframes slidown{
    from{
        transform:  translateY(-100%);
    }
    to{
        transform:  translateY(0%);
    }
}
.slidown{
    display: block;
    animation: slidown .7s ease-in both;
}
@keyframes slidup{
    from{
        transform:  translateY(0%);
    }
    to{
        transform:  translateY(-100%);
    }
}
.slidup{
    display: block;
    animation: slidup .7s ease-in both;
}
.disappear{
    display: none;
}
.qy view{
    text-overflow: ellipsis;
    overflow: hidden; 
    white-space:nowrap;  
    padding-left: 15rpx; 
}
.quyu-left{
      width: 18%;
    background: #c5c5c5;
}
.quyu-center{
      width: 37%;
    background: #e0e0e0;
}
.quyu-right{
      width: 45%;
    background: #e0e0e0;
}
.quyu-right view{
    border-bottom: 1px solid #c5c5c5;
}
.current{
    background: #e0e0e0;
}
.current2{
    background: #fff;
}
.nav-son.active .content{
    color: #61beff;
}
.nav-son.active .icon{
    border-bottom: 4px solid #61beff;
    border-top:0;
}

.slidown{
    display: block;
}

.slidup{
    display: none;

}






