/* pages/service/service.wxss */
.service{
  width:100%;
  height: 200px;
}
.call{
  height:80rpx;
  background:#fff;
  line-height:80rpx;
  padding:0 30rpx;
  font-size:26rpx;
  color:#333;
  position:relative;
}
.call>image{
  width:42rpx;
  height:42rpx;
  position:absolute;
  top:20rpx;
  right:30rpx;
}
.contact_btn{
  position: fixed;
  bottom:0;
  left: 0;
  height: 80rpx;
  width: 100%;
  background: #2FE1D6;
  font-size: 28rpx;
  color: #fff;
  border-radius:0;
  border: none; 
}